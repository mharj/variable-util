{
    "name": "@avanio/variable-util",
    "version": "0.0.1",
    "description": "Utility to get variables from multiple resources",
    "main": "./dist/index.js",
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "test": "mocha"
    },
    "files": [
        "dist"
    ],
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
