{
    "name": "@avanio/variable-util",
    "version": "0.2.19",
    "description": "Utility to get variables from multiple resources",
    "main": "./dist/index.js",
    "private": false,
    "scripts": {
        "doc": "typedoc",
        "build": "tsc",
        "watch": "tsc -w",
        "prepublishOnly": "npm run build",
        "test": "mocha",
        "lint": "eslint src"
    },
    "files": [
        "dist"
    ],
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporters": [
            "spec",
            "mocha-junit-reporter"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mharj/variable-util.git"
    },
    "keywords": [
        "env",
        "variables"
    ],
    "author": "mharj",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mharj/variable-util/issues"
    },
    "homepage": "https://github.com/mharj/variable-util#readme",
    "devDependencies": {
        "@types/chai": "^4.3.5",
        "@types/chai-as-promised": "^7.1.5",
        "@types/mocha": "^10.0.1",
        "@types/node": "^16.18.38",
        "@types/sinon": "^10.0.15",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "chai": "^4.3.7",
        "chai-as-promised": "^7.1.1",
        "cross-fetch": "^4.0.0",
        "dotenv": "^16.3.1",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-deprecation": "^1.4.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-sonarjs": "^0.19.0",
        "mocha": "^10.2.0",
        "prettier": "^2.8.8",
        "sinon": "^15.2.0",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.1",
        "typedoc": "^0.24.8",
        "typescript": "^5.1.6",
        "zod": "^3.21.4"
    },
    "dependencies": {
        "@avanio/logger-like": "^0.1.1",
        "mharj-result": "^0.3.2",
        "url-polyfill": "^1.1.12"
    }
}
