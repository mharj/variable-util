    "description": "Utility to get variables from multiple resources",
    "main": "./dist/index.js",
    "private": false,
    "scripts": {
        "doc": "typedoc",
        "build": "tsc",
        "watch": "tsc -w",
        "prepublishOnly": "npm run build",
        "test": "mocha",
        "lint": "eslint src"
    },
    "files": [
        "dist"
    ],
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporters": [
            "spec",
            "mocha-junit-reporter"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mharj/variable-util.git"
    },
    "keywords": [
