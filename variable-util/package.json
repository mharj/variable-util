{
    "name": "@avanio/variable-util",
    "version": "0.8.0",
    "description": "Utility to get variables from multiple resources",
    "main": "./dist/index.js",
    "private": false,
    "scripts": {
        "doc": "typedoc",
        "build": "tsc",
        "watch": "tsc -w",
        "prepublishOnly": "npm run build",
        "test": "mocha",
        "lint": "eslint src"
    },
    "files": [
        "dist"
    ],
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporters": [
            "spec",
            "mocha-junit-reporter"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mharj/variable-util.git"
    },
    "keywords": [
        "env",
        "variables"
    ],
    "author": "mharj",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mharj/variable-util/issues"
    },
    "homepage": "https://github.com/mharj/variable-util#readme",
    "devDependencies": {
        "@types/chai": "^4.3.14",
        "@types/chai-as-promised": "^7.1.8",
        "@types/etag": "^1.8.3",
        "@types/mocha": "^10.0.6",
        "@types/node": "^18.19.31",
        "@types/sinon": "^17.0.3",
        "@typescript-eslint/eslint-plugin": "^7.7.1",
        "@typescript-eslint/parser": "^7.7.1",
        "chai": "^4.4.1",
        "chai-as-promised": "^7.1.1",
        "cross-fetch": "^4.0.0",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-deprecation": "^2.0.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-sonarjs": "^0.25.1",
        "etag": "^1.8.1",
        "mocha": "^10.4.0",
        "prettier": "^3.2.5",
        "sinon": "^17.0.1",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "typedoc": "^0.25.13",
        "typescript": "^5.4.5",
        "zod": "^3.23.4"
    },
    "dependencies": {
        "@avanio/logger-like": "^0.1.1",
        "@luolapeikko/result-option": "^0.5.3",
        "url-polyfill": "^1.1.12"
    }
}
