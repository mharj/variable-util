{
	"name": "@avanio/variable-util-tachyon",
	"version": "1.3.1",
	"description": "tachyon store for variables",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		},
		"default": "./dist/index.mjs"
	},
	"private": false,
	"scripts": {
		"build": "tsdown src/index.ts --sourcemap --format cjs,esm --dts --clean -c tsconfig.build.json",
		"validate": "tsc --noEmit"
	},
	"repository": "github:mharj/variable-util",
	"files": [
		"dist"
	],
	"author": "mharj",
	"license": "MIT",
	"keywords": [
		"tachyon",
		"storage",
		"env",
		"variable",
		"util"
	],
	"devDependencies": {
		"@avanio/variable-util": "workspace:*",
		"tachyon-drive": "^2.0.0"
	},
	"peerDependencies": {
		"@avanio/logger-like": ">= 0.0.1",
		"@avanio/variable-util": ">= 1.2.1",
		"tachyon-drive": ">= 2.0.0",
		"zod": ">= 3.0.0"
	}
}
