{
	"name": "@avanio/variable-util-node",
	"version": "1.2.2",
	"description": "nodejs env util",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		"types": {
			"import": "./dist/index.d.mts",
			"require": "./dist/index.d.cts"
		},
		"import": "./dist/index.mjs",
		"require": "./dist/index.cjs",
		"default": "./dist/index.mjs"
	},
	"private": false,
	"scripts": {
		"build": "tsdown src/index.ts --sourcemap --format cjs,esm --dts --clean -c tsconfig.build.json",
		"validate": "tsc --noEmit"
	},
	"files": [
		"dist"
	],
	"repository": "github:mharj/variable-util",
	"author": "mharj",
	"license": "MIT",
	"devDependencies": {
		"@avanio/variable-util": "workspace:*"
	},
	"peerDependencies": {
		"@avanio/logger-like": ">= 0.1.0",
		"@avanio/variable-util": ">= 1.2.1",
		"@luolapeikko/result-option": ">= 1.0.0",
		"@luolapeikko/ts-common": ">= 1.0.0",
		"dotenv": ">= 16.0.0"
	}
}
